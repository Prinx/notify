{
    "name": "prinx/notify",
    "description": "Simple log package",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/prinx/notify",
    "authors": [
        {
            "name": "Prince Dorcis",
            "email": "princedorcis@gmail.com",
            "role": "Maintainer",
            "homepage": "https://prinx.github.io"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Prinx\\Notify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}